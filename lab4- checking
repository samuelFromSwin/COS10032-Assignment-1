#include <iostream>
using namespace std; 

int main()
{
    int score; 

    cout << "Enter your score: ";
    cin >> score;

    if (score >= 80 && score <= 100)
    {
        cout << "Grade HD" << endl; 
    }
    else if (score >= 70 && score <= 79)
    {
        cout << "Grade D" << endl; 
    }
    else if (score >= 60 && score <= 69)
    {
        cout << "Grade C" << endl; 
    }
    else if (score >= 50 && score <= 59)
    {
        cout << "Grade P" << endl; 
    }
    else if (score >= 0 && score <= 49)
    {
        cout << "Grade F" << endl; 
    }
    else
    {
        cout << "Invalid score" << endl;
    }

    return 0;
}
